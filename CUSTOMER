class PropertyMgmtSyst
{
public static void main(String args[])
{
Customer cust=new Customer();
Buyer buy=new Buyer();
switch(ch)
{
case 1:
{cust.login();
break;}
case 2:
{cust.register();
break;}
case 3:
{buy.checkList();
break;}
case 4:
{buy.checkLoanOptions();
break;}
case 5:
{cust.giveSuggestion();
break;}
case 6:
break;
}
}while(ch!=6);
}
}
